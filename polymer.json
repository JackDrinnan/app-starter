{
  "entrypoint": "index.html",
  "shell": "src/app-shell.html",
  "fragments": [
    "src/pages/users/users-page.html",
    "src/pages/user/user-page.html",
    "src/pages/login-register/login-register-page.html",
    "src/pages/feedback/feedback-page.html",
    "src/pages/admin/admin-page.html",
    "src/pages/test/test-page.html",

    "src/pages/users/user-list.html",
    "src/pages/users/user-map.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "firebase-messaging-sw.js",
    "bower_components/webcomponentsjs/webcomponents-lite.min.js"
  ],
  "lint": {
    "rules": ["polymer-1"]
  },
  "builds": [{
    "name": "bundled-es6",
    "bundle": true,
    "insertPrefetchLinks": false,
    "addServiceWorker": true,
    "swPrecacheConfig": "sw-precache-config.js",
    "js": {"minify": true},
    "css": {"minify": true},
    "html": {"minify": true}
  },{
    "name": "bundled-es5",
    "bundle": true,
    "insertPrefetchLinks": false,
    "addServiceWorker": true,
    "swPrecacheConfig": "sw-precache-config.js",
    "js": {"minify": true, "compile": true},
    "css": {"minify": true},
    "html": {"minify": true}
  }]
}
